{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "ignorePatterns": [".github/**/*"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "unicorn/filename-case": "off",
    "unicorn/prefer-node-protocol": "off",
    "unicorn/numeric-separators-style": "off",
    "no-console": "off",
    "node/no-missing-import": "off",
    "complexity": "off"
  }
}
